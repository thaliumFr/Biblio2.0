{% extends 'base.html.twig' %}

{% block title %}
  Release
{% endblock %}

{% block body %}
  <h1>Release</h1>

  <table class="table">
    <tbody>
      <tr>
        <th>Id</th>
        <td>{{ release.id }}</td>
      </tr>
      <tr>
        <th>ReleasedAt</th>
        <td>{{ release.releasedAt|date('d-m-Y') }}</td>
      </tr>
      <tr>
        <th>Title</th>
        <td>{{ release.title }}</td>
      </tr>
      <tr>
        <th>ThumbnailUrl</th>
        <td>{{ release.thumbnailUrl }}</td>
      </tr>
      <tr>
        <th>Type</th>
        <td>{{ release.type }}</td>
      </tr>
    </tbody>
  </table>

  <a href="{{ path('app_release_index') }}">back to list</a>

  <a href="{{ path('app_release_edit', { id: release.id }) }}">edit</a>

  {{ include('release/_delete_form.html.twig') }}
{% endblock %}symphony
